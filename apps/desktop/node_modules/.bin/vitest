#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cavatina/projects/aicooper/node_modules/.pnpm/vitest@3.2.4_@types+node@24.0.15_jiti@2.4.2_jsdom@26.1.0_lightningcss@1.30.1_tsx@4.20.3/node_modules/vitest/node_modules:/Users/cavatina/projects/aicooper/node_modules/.pnpm/vitest@3.2.4_@types+node@24.0.15_jiti@2.4.2_jsdom@26.1.0_lightningcss@1.30.1_tsx@4.20.3/node_modules:/Users/cavatina/projects/aicooper/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cavatina/projects/aicooper/node_modules/.pnpm/vitest@3.2.4_@types+node@24.0.15_jiti@2.4.2_jsdom@26.1.0_lightningcss@1.30.1_tsx@4.20.3/node_modules/vitest/node_modules:/Users/cavatina/projects/aicooper/node_modules/.pnpm/vitest@3.2.4_@types+node@24.0.15_jiti@2.4.2_jsdom@26.1.0_lightningcss@1.30.1_tsx@4.20.3/node_modules:/Users/cavatina/projects/aicooper/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
